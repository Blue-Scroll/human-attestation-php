{
  "name": "bluescroll/human-attestation",
  "description": "Official SDK for HAP (Human Attestation Protocol) - cryptographic proof of verified human effort",
  "version": "0.3.3",
  "type": "library",
  "license": "Apache-2.0",
  "homepage": "https://github.com/Blue-Scroll/hap",
  "keywords": [
    "hap",
    "human-attestation-protocol",
    "attestation",
    "verification",
    "proof-of-effort",
    "cryptographic",
    "ed25519",
    "jws",
    "sender-verification"
  ],
  "authors": [
    {
      "name": "BlueScroll Inc."
    }
  ],
  "require": {
    "php": ">=8.1",
    "guzzlehttp/guzzle": "^7.8",
    "web-token/jwt-signature": "^3.2",
    "web-token/jwt-signature-algorithm-eddsa": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.4",
    "phpstan/phpstan": "^1.10"
  },
  "autoload": {
    "psr-4": {
      "BlueScroll\\Hap\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BlueScroll\\Hap\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "analyse": "phpstan analyse"
  }
}
